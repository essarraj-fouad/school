<h1><%= t('.title') %></h1>

<%= form_for @student do |f| %>
  <%= f.label :name %>: <%= f.text_field :name %>
  <%= f.label :classroom %>: <%= f.collection_select :classroom_id, Classroom.all, :id, :level_with_sublevel, prompt: true %>
  <%= f.submit %>
<% end %>

<p>
<%= link_to t('.home'), root_path %>
</p>
